<context>
  <panel label="DFF"
         dxincr="1"
         backgroundcolor="White"
         frequency="months">

    <chart label="S&amp;P 500"
           ngridlines="16"
           span="60"
           scale="log">

      <series ref="SP500"/>
      <series ref="RC" color="Orange"/>
    </chart>

    <chart label="Gross Domestic Product, YoY Percentage Change"
           ngridlines="18"
           span="25">

      <series ref="GDP.pc1"/>
      <series ref="RC" color="Orange"/>
    </chart>

    <chart label="Federal Funds Effective Rate"
           span="15"
           ngridlines="10">

      <series ref="DFF" color="Red"/>
      <series ref="RC" color="Orange"/>
    </chart>
  </panel>
</context>

