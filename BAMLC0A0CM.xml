    <chart label="Gross Domestic Product, YoY Percentage Change"
           ngridlines="18"
           span="25">

      <series ref="GDP.pc1"/>
      <series ref="RC" color="LightCyan"/>
    </chart>

    <chart label="ICE BofA US Corporate Index Option-Adjusted Spread"
           span="15"
           ngridlines="6">

      <series ref="BAMLC0A0CM" color="Red"/>
      <series ref="RC" color="LightCyan"/>
    </chart>
  </panel>
</context>

