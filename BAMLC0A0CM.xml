<context>
  <panel label="ICE BofA Credit Spread"
         dxincr="1"
         backgroundcolor="LightYellow"
         frequency="months">

    <chart label="S&amp;P 500"
           ngridlines="16"
           span="60"
           scale="log">

      <series ref="SP500"/>
      <series ref="RC" color="LightCyan"/>
